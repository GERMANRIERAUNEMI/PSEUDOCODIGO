	SiNo
		DIF_HORAS<-HORAS-35;
		TARIFA_EXTRA<-(TARIFA*1.5);
		SALARIO_EXTRA<-TARIFA_EXTRA*DIF_HORAS;
		SUELDO<-(35*TARIFA)+SALARIO_EXTRA;
