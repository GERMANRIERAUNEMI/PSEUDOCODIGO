	DEFINIR HORAS,SUELDO,DIF_HORAS,TARIFA_EXTRA,SALARIO_EXTRA,SUELDO_MES,IMPUESTO,SUELDO_TOTAL Como REAL;
	TARIFA<-50;
	ESCRIBIR "INTRODUCE LAS HORAS DE TRABAJO ==>";
	LEER HORAS;
	SI HORAS<=35 Entonces
